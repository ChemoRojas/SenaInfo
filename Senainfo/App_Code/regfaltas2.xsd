                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  t1.ICodFalta, 
		t1.ICodIE, 
		t1.FechaEventoFalta, 
		t1.EventoDescripcionFalta, 
		t1.PresentaDenunciaFalta_Afirmacion, 
		isnull(t2.Descripcion,'-') as FaltaCF, 
		isnull(t3.Descripcion,'-') as Falta1,
		isnull(t4.Descripcion,'-') as Falta2,
		isnull(t5.Descripcion,'-') as Falta3,
		isnull(t6.Descripcion,'-') as Falta4,
		
		case t7.Descripcion
		when 'SIN INFORMACION' then 'S/I' else isnull(t7.Descripcion,'-') end as Sancion1,

		case t8.Descripcion
		when 'SIN INFORMACION' then 'S/I' else isnull(t8.Descripcion,'-') end as Sancion2,

		case t9.Descripcion
		when 'SIN INFORMACION' then 'S/I' else isnull(t9.Descripcion,'-') end as Sancion3,

		case t10.Descripcion
		when 'SIN INFORMACION' then 'S/I' else isnull(t10.Descripcion,'-') end as Sancion4,

		case t10.Descripcion
	    when 'SIN INFORMACION' then 'S/I' else isnull(t10.Descripcion,'-') end as Conflicto

  FROM  Faltas t1 LEFT JOIN 
        parTipoFaltas as t2 ON t1.CodTipoFaltaCF = t2.CodTipoFalta LEFT JOIN
        parTipoFaltas as t3 ON t1.codfalta1cf = t3.CodTipoFalta LEFT JOIN
		parTipoFaltas as t4 ON t1.codfalta2cf = t4.CodTipoFalta LEFT JOIN
		parTipoFaltas as t5 ON t1.codfalta3cf = t5.CodTipoFalta LEFT JOIN
		parTipoFaltas as t6 ON t1.codfalta4cf = t6.CodTipoFalta LEFT JOIN
		parSancionFalta as t7 ON t1.CodSancionFalta1CF = t7.CodSancionFalta LEFT JOIN
		parSancionFalta as t8 ON t1.CodSancionFalta2CF = t8.CodSancionFalta LEFT JOIN
		parSancionFalta as t9 ON t1.CodSancionFalta3CF = t9.CodSancionFalta LEFT JOIN
		parSancionFalta as t10 ON t1.CodSancionFalta4CF = t10.CodSancionFalta LEFT JOIN 
		parConflictoCritico as t11 ON t1.ConstituyeCC_Afirmacion = t11.codConflictoCritico

where t1.IcodIE = @icodie
  </CommandText>
